{
  "name": "自定义",
  "version": 100,
  "category": 0,
  "url": "http://www.yeyexs.cc",
  "charset": "utf-8",
  "metadata": {
    "name": ["li > a", "#list > dl > dt@js->${this}.match('《(.*)》')[1]"],
    "author": ["li@js->${this}.match('</a>(.*)$')[1].replace('</a>','')", "#info .infotitle > i@js->${this}.match('作者：(.*)')[1]"],
    "cover": ["#fmimg > a > img@attr->src"],
    "summary": ["#info .intro"],
    "category": ["#info .infotitle > i@js->${this}.match('类别：(.*)')[1]"],
    "status": ["#info .infotitle > i@js->${this}.match('状态：(.*)')[1]"],
    "update": [".book_info > .infos > span > span@js->${this}.match('最后更新：(.*)状态')[1]"],
    "lastChapter": ["#info .bookurl > p > a"],
    "link": ["li > a@attr->href"],
    "catalog": []
  },
  "catalog": {
    "list": "#list > dl > dd",
    "orderBy": 0,
    "chapter": {
      "name": "a",
      "link": "a@attr->href"
    }
  },
  "content": {
    "text": "div#BookText",
    "next": {
      "link": "div.bottem > a",
      "text": "下一页"
    }
  },
  "search": {
    "link": "http://api.zhuishushenqi.com/book/fuzzy-search?query=${key}",
    "list": "$.books[?(@.contentType =='txt')]"
  },
  "rank": {
    "link": [
      {
        "name": "大全列表",
        "link": "http://www.yeyexs.cc/xiaoshuodaquan/"
      }
    ],
    "list": ".novellist > ul"
  }
}
